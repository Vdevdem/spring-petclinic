pipeline {
    agent any

    stages {

        stage('Checkout') {
          //Copies git repo
          steps {
              git branch: 'main', credentialsId: 'Git_FProject', url: 'https://github.com/Vdevdem/spring-petclinic.git'
          }

          stage('hello') {
            steps {
                sh "echo 'hello"
            }
          }
      }
   }
}
